<?xml version="1.0" encoding="utf-8"?>
<!-- vim:fdn=3:
-->
<openerp>
	<data>

		<!-- Move jQuery UI -->
		<template id="ot_assets_editor" inherit_id="website.assets_editor" name="OT Assets Editor">
			<xpath expr="//link[@href='/web/static/lib/jquery.ui/css/smoothness/jquery-ui-1.9.1.custom.css']" position="replace"/>
			<xpath expr="//t[@t-call='web.jqueryui_conflict']" position="replace"/>
		</template>

		<!-- Insert CSS & Javascript -->
		<template id="ot_assets_frontend" inherit_id="website.assets_frontend" name="OT Assets">
			<xpath expr="." position="inside">
				<link rel='stylesheet' href="/web/static/lib/jquery.ui/css/smoothness/jquery-ui-1.9.1.custom.css"/>
				<link rel="stylesheet" href="/ot_fornest/static/src/css/jquery.bxslider.css"/>
				<link rel="stylesheet" href="/ot_fornest/static/src/css/style.css"/>
				<script type="text/javascript" src="/ot_fornest/static/src/js/jquery.bxslider.js"/>
				<t t-call="web.jqueryui_conflict">
					<script type="text/javascript" src="/web/static/lib/jquery.ui/js/jquery-ui-1.9.1.custom.js"></script>
				</t>
			</xpath>
		</template>

		<!-- Insert Custom Script After website_sale.js -->
		<template id="ot_ws_assets_frontend" inherit_id="website_sale.assets_frontend" name="OT WS Assets">
			<xpath expr="." position="inside">
				<script type="text/javascript" src="/ot_fornest/static/src/js/script.js"/>
			</xpath>
		</template>

		<!-- Change Logo -->
		<template id="website.layout_logo_show" inherit_id="website.layout" customize_show="True" name="Show Logo">
			<xpath expr="//header//a[@class='navbar-brand']" position="replace">
				<a href="/" class="navbar-brand logo">
					<img src="/ot_fornest/static/src/img/logo.png" t-att-alt="'Logo of %s' % res_company.name" t-att-title="res_company.name"/>
				</a>
			</xpath>
		</template>

		<!-- Change Sign In Position -->
		<template id="website.show_sign_in" customize_show="True" inherit_id="website.layout" name="Show Sign In" groups="base.group_public">
			<xpath expr="//ul[@id='top_menu']" position="inside">
				<li>
					<a t-attf-href="/web/login">
						<i class="fa fa-sign-in"></i>
						<b>Sign in</b>
					</a>
				</li>
			</xpath>
		</template>

		<!-- Allow Search in Any Page -->
		<template id="website_sale.search" name="Search hidden fields">
			<t t-if="category">
				<form t-att-action="keep('/shop'+ ('/category/'+slug(category)) if category else '', search=0)" method="get" t-att-class="search_class">

					<t t-if="attrib_values">
						<t t-foreach="attrib_values" t-as="a">
							<input type="hidden" name="attrib" t-att-value="'%s-%s' % (a[0], a[1])"/>
						</t>
					</t>
					<div class="input-group">
						<input type="text" name="search" class="search-query form-control" placeholder="Search..." t-att-value="search"/>
						<span class="input-group-btn">
							<a class="btn btn-default a-submit"><i class="fa fa-search"/></a>
						</span>
					</div>
				</form>
			</t>

			<t t-if="not category">
				<form action="/shop" method="get" t-att-class="search_class">

					<t t-if="attrib_values">
						<t t-foreach="attrib_values" t-as="a">
							<input type="hidden" name="attrib" t-att-value="'%s-%s' % (a[0], a[1])"/>
						</t>
					</t>
					<div class="input-group">
						<input type="text" name="search" class="search-query form-control" placeholder="Search..." t-att-value="search"/>
						<span class="input-group-btn">
							<button type="submit" class="btn btn-default a-submit"><i class="fa fa-search"/></button>
						</span>
					</div>
				</form>
			</t>
		</template>

		<!-- Change Main Layout -->
		<template id="ot_layout" inherit_id="website.layout" name="OT Layout" priority="1">
			<xpath expr="//head" position="inside">
				<link rel="shortcut icon" href="/ot_fornest/static/src/img/favicon.ico" type="image/x-icon"/>
			</xpath>
			<xpath expr="//div[@class='navbar navbar-default navbar-static-top']" position="replace">
				<div class="navbar navbar-default navbar-static-top ot-fixed-header">
					<div class="container">
						<div class="navbar-header col-lg-3 col-md-3 col-sm-12 col-xs-12">
							<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-top-collapse">
									<span class="sr-only">Toggle navigation</span>
									<span class="icon-bar"></span>
									<span class="icon-bar"></span>
									<span class="icon-bar"></span>
							</button>
							<a class="navbar-brand" href="/" t-field="res_company.name"/>
						</div>
						<t t-call="website_sale.search">
							<t t-set="search_class">ot-navbar-search col-lg-4 col-md-3 col-sm-5 col-xs-12</t>
						</t>

						<div class="collapse navbar-collapse navbar-top-collapse ot-sub-menu">
							<ul class="nav navbar-nav navbar-right col-lg-5 col-md-6 col-sm-7 col-xs-12" id="top_menu">
								<li></li>
								<t t-if="website.user_id != user_id">
									<li>
										<a href="/web">
											<i class="fa fa-user"></i>
											My Account (<t t-esc="user_id.name"/>)
										</a>
									</li>
									<li>
										<a t-attf-href="/web/session/logout?redirect=/">
											<i class="fa fa-sign-out"></i>
											Logout
										</a>
									</li>
								</t>
							</ul>
						</div>
					</div>
				</div>

				<div class="navbar navbar-default navbar-static-top">
					<div class="container">
						<div class="clearfix"/>

						<div class="collapse navbar-collapse navbar-top-collapse ot-main-menu">
							<ul class="nav navbar-nav">
								<t t-foreach="website.menu_id.child_id" t-as="submenu">
									<t t-call="website.submenu"/>
								</t>
							</ul>
						</div>
					</div>
				</div>
			</xpath>

			<xpath expr="//div[@id='wrapwrap']" position="after">
				<a href="#" class="ot-top">Top</a>
			</xpath>
		</template>

		<!-- Change Cart Position -->
		<template id="website_sale.header" inherit_id="website.layout" name="Header Shop My Cart Link" priority="2">
			<xpath expr="//header//ul[@id='top_menu']/li" position="replace">
				<t t-set="website_sale_order" t-value="website.sale_get_order()"/>
				<li class="ot-my-cart">
					<a href="#" class="dropdown-toggle" data-toggle="dropdown">
						<img src="/ot_fornest/static/src/img/cart.png" class="img img-responsive" alt="OT Cart"/> 
						<sup t-attf-class="my_cart_quantity label label-primary" t-esc="website_sale_order and website_sale_order.cart_quantity or '0'"/>
					</a>
					<ul t-att-class="'dropdown-menu ot-cart-content' if website.user_id != user_id else 'dropdown-menu ot-cart-content ot-left-cart'">
						<t t-if="website_sale_order">
							<t t-if="website_sale_order.order_line">
								<div class="ot-cart-title">
									<h4>My Cart</h4>
								</div>
								<div class="dropdown-cart">
									<t t-foreach="website_sale_order.order_line" t-as="line">
										<span align="center" t-if="line.product_id.product_tmpl_id">
											<span class="ot-image-cart" t-field="line.product_id.image_small" t-field-options='{"widget": "image", "class": "img-rounded"}'/>
										</span>
										<span t-if="line.product_id.product_tmpl_id">
											<span style="display: block">
												<a t-attf-href="/shop/product/#{ slug(line.product_id.product_tmpl_id) }">
													<strong t-esc="line.product_id.name_get()[0][1]"/>
												</a>

												<form action="/shop/cart/update" method="post" style="display: inline-block;">
													<input name="remove_product_id" t-att-value="line.product_id.id" type="hidden"/>
													<a class="ot-remove-product"><i class="fa fa-times"/></a>
												</form>
											</span>
										</span>
										<span class="text-center" name="price">
											<t t-if="(line.price_unit - line.price_reduce) &gt; 1">
												<del class="text-danger" style="white-space: nowrap;" t-field="line.price_unit" t-field-options='{"widget": "monetary", "from_currency": "website.currency_id", "display_currency": "user_id.partner_id.property_product_pricelist.currency_id"}'/>&amp;nbsp;
											</t>
											<span t-esc="int(line.product_uom_qty)"/>
											<span> x </span>
											<span t-field="line.price_reduce" style="white-space: nowrap;" t-field-options='{"widget": "monetary", "display_currency": "user_id.partner_id.property_product_pricelist.currency_id"}'/>
										</span>
										<div class="clearfix"/>
									</t>
								</div>

								<div class="ot-total-amount">
									<h4>Total: <span t-field="website_sale_order.amount_total" style="white-space: nowrap;" t-field-options='{"widget": "monetary", "display_currency": "user_id.partner_id.property_product_pricelist.currency_id"}'/></h4>
								</div>

								<div class="ot-cart-action">
									<a href="/shop/cart"><i class="fa fa-shopping-cart"/> My Cart</a>
									<a t-if="not optional_products and website_sale_order and website_sale_order.website_order_line" href="/shop/checkout"><i class="fa fa-sign-out"/> Checkout</a>
								</div>
							</t>
							<t t-if="not website_sale_order.order_line">
								<div class="ot-cart-empty dropdown-cart">
									<span>Your cart was empty.</span>
								</div>
							</t>
						</t>

						<t t-if="not website_sale_order">
							<div class="ot-cart-empty dropdown-cart">
								<span>Your cart was empty.</span>
							</div>
						</t>
					</ul>
				</li>
			</xpath>
		</template>

		<!-- Homepage -->
		<template id="website.homepage" name="Homepage">
			<t t-call="website.layout">
				<div id="wrap" class="js_sale oe_website_sale">
					<div class="oe_structure"/>

					<div class="carousel slide" id="ot-main-slider">
						<ol class="carousel-indicators hidden-xs">
							<li class="active" data-slide-to="0" data-target="#ot-main-slider"/>
							<li data-slide-to="1" data-target="#ot-main-slider"/>
						</ol>
						<!-- <hr class="transition-timer-carousel-progress-bar" /> -->
						<div class="carousel-inner" role="listbox">
							<div class="item active">
								<a href="/">
									<img src="/ot_fornest/static/src/img/slide-1.jpg" alt="OT Slide"/>
								</a>
							</div>
							<div class="item">
								<a href="/">
									<img src="/ot_fornest/static/src/img/slide-blank.png" alt="OT Blank Slide"/>
								</a>
							</div>
						</div>
				
						<div class="carousel-control left" data-slide="prev" data-target="#ot-main-slider" href="#ot-main-slider" style="width: 10%">
							<i class="fa fa-chevron-left"/>
						</div>
						<div class="carousel-control right" data-slide="next" data-target="#ot-main-slider" href="#ot-main-slider" style="width: 10%">
							<i class="fa fa-chevron-right"/>
						</div>
					</div>

					<div class="oe_structure"/>

					<div class="ot-sale-row">
						<div class="container">
							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
								<img src="/ot_fornest/static/src/img/shop-1.jpg" alt="Shop Discount" class="img img-responsive"/>
							</div>

							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
								<img src="/ot_fornest/static/src/img/shop-2.jpg" alt="Shop Brand" class="img img-responsive"/>
							</div>
						</div>
					</div>

					<div class="oe_structure"/>

					<div class="ot-featured-brand">
						<div class="container text-center">
							<h3>Featured Brand</h3>
							<ul>
								<li><img src="/ot_fornest/static/src/img/brand-1.png" alt="Brand 1" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-2.png" alt="Brand 2" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-3.png" alt="Brand 3" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-4.png" alt="Brand 4" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-5.png" alt="Brand 5" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-6.png" alt="Brand 6" class="img img-responsive"/></li>
							</ul>
							<ul>
								<li><img src="/ot_fornest/static/src/img/brand-7.png" alt="Brand 7" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-8.png" alt="Brand 8" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-9.png" alt="Brand 9" class="img img-responsive"/></li>
								<li><img src="/ot_fornest/static/src/img/brand-10.png" alt="Brand 10" class="img img-responsive"/></li>
							</ul>
						</div>
					</div>

					<div class="oe_structure"/>
					
					<div class="carousel slide" id="ot-sub-slider">
						<ol class="carousel-indicators hidden-xs">
							<li class="active" data-slide-to="0" data-target="#ot-sub-slider"/>
							<li data-slide-to="1" data-target="#ot-sub-slider"/>
						</ol>
						<!-- <hr class="transition-timer-carousel-progress-bar" /> -->
						<div class="carousel-inner" role="listbox">
							<div class="item active">
								<a href="/">
									<img src="/ot_fornest/static/src/img/banner-1.png" alt="OT Sub Slide"/>
								</a>
							</div>
							<div class="item">
								<a href="/">
									<img src="/ot_fornest/static/src/img/banner-blank.png" alt="OT Blank Sub Slide"/>
								</a>
							</div>
						</div>
				
						<div class="carousel-control left" data-slide="prev" data-target="#ot-sub-slider" href="#ot-sub-slider" style="width: 10%">
							<i class="fa fa-chevron-left"/>
						</div>
						<div class="carousel-control right" data-slide="next" data-target="#ot-sub-slider" href="#ot-sub-slider" style="width: 10%">
							<i class="fa fa-chevron-right"/>
						</div>
					</div>

					<div class="oe_structure"/>
					<t t-if="ot_categ_1 or ot_categ_2 or ot_categ_3">
						<div class="ot-products">
							<div class="container" id="ot-products-tabs">
								<h3 class="text-center">Our Products</h3>
								<ul class="text-center">
									<t t-if="ot_categ_1">
										<li><a href="#ot-products-tabs-1"><t t-esc="company.ot_categ_1.name"/></a></li>
									</t>
									<t t-if="ot_categ_2">
										<li><a href="#ot-products-tabs-2"><t t-esc="company.ot_categ_2.name"/></a></li>
									</t>
									<t t-if="ot_categ_3">
										<li><a href="#ot-products-tabs-3"><t t-esc="company.ot_categ_3.name"/></a></li>
									</t>
								</ul>

								<div id="products_grid">
									<t t-if="ot_categ_1">
										<div id="ot-products-tabs-1">
											<table width="100%" class="text-center">
											<tbody>
												<tr t-foreach="ot_categ_1" t-as="tr_product">
												<t t-foreach="tr_product" t-as="td_product">
												<t t-if="td_product">
													<t t-set="product" t-value="td_product['product']" />
													<td t-att-colspan="td_product['x'] != 1 and td_product['x']" t-att-rowspan="td_product['y'] != 1 and td_product['y']" t-attf-class="oe_product oe_grid oe-height-#{td_product['y']*2} #{ td_product['class'] }">
													<div class="oe_product_cart" t-att-data-publish="product.website_published and 'on' or 'off'">
														<div class="css_options" t-ignore="true" groups="base.group_website_publisher">
														<div t-attf-class="dropdown js_options" t-att-data-id="product.id">
															<button class="btn btn-default" t-att-id="'dopprod-%s' % product.id" role="button" data-toggle="dropdown">
															Options
															<span class="caret" />
															</button>
															<ul class="dropdown-menu" role="menu" t-att-aria-labelledby="'dopprod-%s' % product.id">
															<li class="dropdown-submenu">
																<a tabindex="-1" href="#">Size</a>
																<ul class="dropdown-menu" name="size">
																<li>
																	<a href="#">
																	<table>
																		<tr>
																		<td class="selected" />
																		<td t-att-class="product.website_size_x &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_x &gt; 2 and 'selected'" />
																		<td t-att-class="product.website_size_x &gt; 3 and 'selected'" />
																		</tr>
																		<tr>
																		<td t-att-class="product.website_size_y &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 1 and product.website_size_x &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 1 and product.website_size_x &gt; 2 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 1 and product.website_size_x &gt; 3 and 'selected'" />
																		</tr>
																		<tr>
																		<td t-att-class="product.website_size_y &gt; 2 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 2 and product.website_size_x &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 2 and product.website_size_x &gt; 2 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 2 and product.website_size_x &gt; 3 and 'selected'" />
																		</tr>
																		<tr>
																		<td t-att-class="product.website_size_y &gt; 3 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 3 and product.website_size_x &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 3 and product.website_size_x &gt; 2 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 3 and product.website_size_x &gt; 3 and 'selected'" />
																		</tr>
																	</table>
																	</a>
																</li>
																</ul>
															</li>
															<li class="dropdown-submenu">
																<a tabindex="-1" href="#">Styles</a>
																<ul class="dropdown-menu" name="style">
																<t t-foreach="styles" t-as="style">
																	<li t-att-class="style_in_product(style, product) and 'active' or ''">
																	<a href="#" t-att-data-id="style.id" t-att-data-class="style.html_class">
																		<t t-esc="style.name" />
																	</a>
																	</li>
																</t>
																</ul>
															</li>
															<li class="dropdown-submenu">
																<a tabindex="-1" href="#">Promote</a>
																<ul class="dropdown-menu" name="sequence">
																<li>
																	<a href="#" class="js_go_to_top">Push to top</a>
																</li>
																<li>
																	<a href="#" class="js_go_up">Push up</a>
																</li>
																<li>
																	<a href="#" class="js_go_down">Push down</a>
																</li>
																<li>
																	<a href="#" class="js_go_to_bottom">Push to bottom</a>
																</li>
																</ul>
															</li>
															</ul>
														</div>
														</div>
														<t t-set="product_image_big" t-value="td_product['x']+td_product['y'] &gt; 2" />
														<t t-call="website_sale.products_item" />
													</div>
													</td>
												</t>
												</t>
												</tr>
											</tbody>
											</table>
										</div>
									</t>

									<t t-if="ot_categ_2">
										<div id="ot-products-tabs-2">
											<table width="100%" class="text-center">
											<tbody>
												<tr t-foreach="ot_categ_2" t-as="tr_product">
												<t t-foreach="tr_product" t-as="td_product">
												<t t-if="td_product">
													<t t-set="product" t-value="td_product['product']" />
													<td t-att-colspan="td_product['x'] != 1 and td_product['x']" t-att-rowspan="td_product['y'] != 1 and td_product['y']" t-attf-class="oe_product oe_grid oe-height-#{td_product['y']*2} #{ td_product['class'] }">
													<div class="oe_product_cart" t-att-data-publish="product.website_published and 'on' or 'off'">
														<div class="css_options" t-ignore="true" groups="base.group_website_publisher">
														<div t-attf-class="dropdown js_options" t-att-data-id="product.id">
															<button class="btn btn-default" t-att-id="'dopprod-%s' % product.id" role="button" data-toggle="dropdown">
															Options
															<span class="caret" />
															</button>
															<ul class="dropdown-menu" role="menu" t-att-aria-labelledby="'dopprod-%s' % product.id">
															<li class="dropdown-submenu">
																<a tabindex="-1" href="#">Size</a>
																<ul class="dropdown-menu" name="size">
																<li>
																	<a href="#">
																	<table>
																		<tr>
																		<td class="selected" />
																		<td t-att-class="product.website_size_x &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_x &gt; 2 and 'selected'" />
																		<td t-att-class="product.website_size_x &gt; 3 and 'selected'" />
																		</tr>
																		<tr>
																		<td t-att-class="product.website_size_y &gt; 1 and 'selected'" />
																		<td t-att-class="product.website_size_y &gt; 1 and product.website_size_x &gt; 1 and 'selected'" />
