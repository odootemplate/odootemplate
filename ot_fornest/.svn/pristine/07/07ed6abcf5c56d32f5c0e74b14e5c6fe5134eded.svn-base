		<!-- Product Attributes Modal -->
		<template id="ot_product_attributes" name="Product Attributes Option">
			<div class="modal ot-modal-attributes fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog modal-lg">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
							<h4 class="modal-title text-center" id="myModalLabel">Product Attributes</h4>
						</div>
						<div class="modal-body">
							<div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 ot-product-images">
								<div class="gallery-thumbs-container col-lg-3 col-md-3 col-sm-3 col-xs-3" t-if="len(product.product_images_ids) &gt; 0">
									<ul id="gallery-thumbs" class="gallery-thumbs-list">
										<li class="thumb-item">
											<div class="thumb">
												<a href="#"><span t-field="product.image" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></a>
											</div>
										</li>
										<t t-foreach="product.product_images_ids" t-as="i">
											<li class="thumb-item">
												<div class="thumb">
													<a href="#"><img t-att-src="'/web/binary/image?model=ot.product.images&amp;field=image&amp;id=' + str(i.id)" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></a>
												</div>
											</li>
										</t>
									</ul>
								</div>
							
								<div t-att-class="'col-lg-9 col-md-9 col-sm-9 col-xs-9' if len(product.product_images_ids) &gt; 0 else 'col-lg-12 col-md-12 col-sm-12 col-xs-12'">
									<ul class="bxslider">
										<li><span t-field="product.image" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></li>
										<t t-foreach="product.product_images_ids" t-as="i">
											<li><img t-att-src="'/web/binary/image?model=ot.product.images&amp;field=image&amp;id=' + str(i.id)" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></li>
										</t>
									</ul>
								</div>
							</div>
							<div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
								<h1 itemprop="name" t-field="product.name">Product Name</h1>
								<span itemprop="url" style="display:none;" t-esc="'/shop/product/%s' % slug(product)"/>

								<t t-if="product.description_sale">
									<div><p t-field="product.description_sale" class="text-muted"/></div>
									<hr t-if="product.description_sale"/>
								</t>
								
								<t t-call="website_sale.product_price"/>
								<hr/>

								<form action="/shop/cart/update" class="js_add_cart_variants" method="POST">

									<div class="js_product">
										<t t-placeholder="select">
											<input type="hidden" class="product_id" name="product_id" t-att-value="int(product.product_variant_ids[0]) if len(product.product_variant_ids) >= 1 else '0'"/>
											<t t-call="website_sale.variants">
												<t t-set="ul_class" t-value="'nav-stacked'"/>
											</t>
										</t>

										<p t-if="len(product.product_variant_ids) > 1" class="css_not_available_msg bg-danger" style="padding: 15px;">Product not available</p>

										<div class="css_quantity input-group oe_website_spinner">
											<input type="text" class="js_quantity form-control" data-min="1" name="add_qty" value="1"/>
											<div class="ot-qty-changer">
												<a t-attf-href="#" class="js_add_cart_json">
													<i class="fa fa-plus"/>
												</a>
												<a t-attf-href="#" class="js_add_cart_json">
													<i class="fa fa-minus"/>
												</a>
											</div>
										</div>

										<a id="add_to_cart" class="btn btn-primary btn-lg js_check_product a-submit ot-add-product" href="#">Add to Cart</a>
									</div>

								</form>
							</div>
							<div class="clearfix"/>
						</div>
					</div>
				</div>
			</div>
		</template>

		<!-- Modal Subscribe -->
		<template id="ot_subscribe" name="Subscribe">
			<div class="modal ot-modal-subscribe fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog modal-lg">
					<div class="modal-content">
						<div class="modal-body text-center">
							<t t-if="is_error">
								<div>Email has already been submited before.</div>
							</t>

							<t t-if="not is_error">
								<div>Your email has been submited.</div>
								<div>HAPPY SHOPPING!</div>
							</t>
						</div>
					</div>
				</div>
			</div>
		</template>

	</data>
</openerp>