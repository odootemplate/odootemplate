		<!-- Single Product Images Slider -->
		<template id="ot_product_images" inherit_id="website_sale.product" name="OT Product Images">
			<xpath expr="//span[@t-field='product.image']" position="replace">
				<div class="gallery-thumbs-container col-lg-3 col-md-3 col-sm-3 col-xs-3" t-if="len(product.product_images_ids) &gt; 0">
					<ul id="gallery-thumbs" class="gallery-thumbs-list">
						<li class="thumb-item">
							<div class="thumb">
								<a href="#"><span t-field="product.image" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></a>
							</div>
						</li>
						<t t-foreach="product.product_images_ids" t-as="i">
							<li class="thumb-item">
								<div class="thumb">
									<a href="#"><img t-att-src="'/web/binary/image?model=ot.product.images&amp;field=image&amp;id=' + str(i.id)" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></a>
								</div>
							</li>
						</t>
					</ul>
				</div>
			
				<div t-att-class="'col-lg-9 col-md-9 col-sm-9 col-xs-9' if len(product.product_images_ids) &gt; 0 else 'col-lg-12 col-md-12 col-sm-12 col-xs-12'">
					<ul class="bxslider">
						<li><span t-field="product.image" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></li>
						<t t-foreach="product.product_images_ids" t-as="i">
							<li><img t-att-src="'/web/binary/image?model=ot.product.images&amp;field=image&amp;id=' + str(i.id)" class="img-responsive" t-field-options="{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-responsive&quot;}"/></li>
						</t>
					</ul>
				</div>
			</xpath>
		</template>

