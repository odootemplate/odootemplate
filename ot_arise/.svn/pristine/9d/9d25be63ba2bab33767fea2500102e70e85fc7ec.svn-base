<?xml version="1.0" encoding="utf-8"?>
<!-- vim:fdn=3:
-->
<openerp>
	<data>

		<!-- Insert CSS & Javascript -->
		<template id="ot_assets_frontend" inherit_id="website.assets_frontend" name="OT Assets">
			<xpath expr="." position="inside">
				<link rel="stylesheet" href="/ot_arise/static/src/css/style.css"/>
				<script type="text/javascript" src="/ot_arise/static/src/js/script.js"/>
			</xpath>
		</template>

		<!-- Change Logo -->
		<template id="website.layout_logo_show" inherit_id="website.layout" customize_show="True" name="Show Logo">
			<xpath expr="//header//a[@class='navbar-brand']" position="replace">
				<a href="/" class="navbar-brand logo">
					<img src="/ot_arise/static/src/img/logo-grey.png" t-att-alt="'Logo of %s' % res_company.name" t-att-title="res_company.name"/>
				</a>
			</xpath>
		</template>

		<!-- Change Main Layout -->
		<template id="ot_layout" inherit_id="website.layout" name="OT Layout" priority="1">
			<xpath expr="//head" position="inside">
				<link rel="shortcut icon" href="/ot_arise/static/src/img/favicon.ico" type="image/x-icon"/>
			</xpath>
			<xpath expr="//div[@class='navbar navbar-default navbar-static-top']" position="before">
				<div class="ot-top-header">
					<div class="container">
						<div class="ot-top-info"><i class="fa fa-phone"/> <span t-field="res_company.phone"/></div>
						<div class="ot-top-info"><i class="fa fa-envelope"/> <span t-field="res_company.email"/></div>
					</div>
				</div>
			</xpath>
		</template>

		<!-- Fixed Header -->
		<template id="ot_fixed_header" inherit_id="website.layout" name="OT Fixed Header" customize_show="True" priority="2">
			<xpath expr="//div[@class='navbar navbar-default navbar-static-top']" position="attributes">
				<attribute name="class">navbar navbar-default navbar-static-top ot-fixed-header</attribute>
			</xpath>
		</template>

		<!-- Subscribe -->
		<template id="ot_subscribe_btn" inherit_id="website.layout" name="OT Subscribe Button" customize_show="True">
			<xpath expr="//ul[@id='top_menu']" position="attributes">
				<attribute name="class">nav navbar-nav</attribute>
			</xpath>
			<xpath expr="//ul[@id='top_menu']" position="after">
				<div class="hidden-lg clearfix"/>
				<a href="#" class="ot-subscribe-btn mt8 mb8"><i class="fa fa-pencil"/> SUBSCRIBE NOW</a>
			</xpath>
		</template>

		<!-- Modal Subscribe -->
		<template id="ot_subscribe_modal" name="OT Subscribe Modal">
			<div class="modal ot-subscribe-modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog modal-lg">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
							<h4 class="modal-title text-center" id="myModalLabel">SUBSCRIPTION</h4>
						</div>
						<div class="modal-body text-center">
							<form action="#" name="subscribe" id="ot-subscribe-form" method="POST">
								<input type="email" required="1" name="email" id="ot-subscribe-email" placeholder="Enter Your E-Mail"/>
								<input type="submit" name="submit" value="Subscribe"/>
							</form>
						</div>
					</div>
				</div>
			</div>
		</template>

		<!-- Subscribe Confirm -->
		<template id="ot_subscribe_confirm" name="OT Subscribe Confirm">
			<div class="modal ot-subscribe-confirm fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog modal-lg">
					<div class="modal-content">
						<div class="modal-body text-center">
							<t t-if="is_error">
								<div>Email has already been submited before.</div>
							</t>

							<t t-if="not is_error">
								<div>Your email has been submited.</div>
							</t>
						</div>
					</div>
				</div>
			</div>
		</template>

